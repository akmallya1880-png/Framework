using OpenQA.Selenium;
using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Automation_Learning_November.Framework
{
    public class BookingConfirmation : SeleniumBase
    {
        public BookingConfirmation(IWebDriver driver) : base(driver)
        {
        }
        By Itinerary = By.Id("my_itinerary");

        public void BookingConfirm()
        {
            ClickElement(Itinerary);
        }
    }
}
